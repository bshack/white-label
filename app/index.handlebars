<!DOCTYPE html>
<html id="index" lang="en-US" dir="ltr">
    {{> tags/head}}
    <body itemscope itemtype="http://schema.org/WebPage">
        <div id="wrapper">
            <main id="main" tabindex="-1" class="container">
                <p>Build your site here.</p>
            </main>
        </div>
        {{#if scripts.global}}
        {{> tags/script script=scripts.global}}
        {{/if}}
        {{#if scripts.section}}
        {{> tags/script script=scripts.section}}
        {{/if}}
        {{#if scripts.page}}
        {{> tags/script script=scripts.page}}
        {{/if}}
    </body>
</html>
