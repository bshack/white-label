<img
    {{#if this.id}}
    id="{{this.id}}"
    {{/if}}
    {{#if this.class}}
    class="{{this.class}}"
    {{/if}}
    {{#if this.aria-label}}
    aria-label="{{this.aria-label}}"
    {{/if}}
    {{#if this.image}}
    src="{{#unless this.external}}{{this.cdn}}{{/unless}}{{this.image}}"
    {{/if}}
    alt="{{#if this.alt}}{{this.alt}}{{/if}}"
>