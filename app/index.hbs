<!DOCTYPE html>
<html id="index" lang="en-US" dir="ltr">
    {{> element/head}}
    <body itemscope itemtype="http://schema.org/WebPage">
        <div id="wrapper">
            <main id="main" tabindex="-1" class="row">
                <div class="small-12 columns">
                    <h1 class="heading-1">Welcome</h1>
                    <p>Build your site here.</p>
                </div>
            </main>
        </div>
        {{#if scripts.global}}
        {{> element/script script=scripts.global}}
        {{/if}}
        {{#if scripts.section}}
        {{> element/script script=scripts.section}}
        {{/if}}
        {{#if scripts.page}}
        {{> element/script script=scripts.page}}
        {{/if}}
    </body>
</html>
